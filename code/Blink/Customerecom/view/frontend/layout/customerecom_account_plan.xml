<page  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd" layout="2columns-left">
     <update handle="customer_account"/>
    <body>
        
            <head>
                <title>Account Information</title>
            </head>
             <referenceBlock name="page.main.title">
                <action method="setPageTitle">
                    <argument translate="true" name="title" xsi:type="string">My Plan</argument>
                </action>
            </referenceBlock>
        <referenceContainer name="content">
           
            <block class="Blink\Customerecom\Block\Account" name="customer.plan.container" template="Blink_Customerecom::plan.phtml"/>
        </referenceContainer>
    </body>
</page>
