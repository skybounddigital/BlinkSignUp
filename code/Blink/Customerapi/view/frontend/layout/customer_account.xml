<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="customer_account_navigation">
            <block class="Blink\Customerapi\Block\Customergroup\Link" name="customer-account-navigation-new-link1" after="-">
                <arguments>
                    <argument name="label" xsi:type="string">Shared Websites</argument>
                    <argument name="path" xsi:type="string">customergen/shared/index</argument>
                </arguments>
            </block>
          
             <block class="Blink\Customerapi\Block\Customergroup\Link" name="customer-account-navigation-new-link190" after="-">
                <arguments>
                    <argument name="label" xsi:type="string">MyQR</argument>
                    <argument name="path" xsi:type="string">customergen/shared/qr</argument>
                </arguments>
            </block>
            
            
             <block class="Blink\Customerapi\Block\Customergroup\Ecomlink" name="customer-account-navigation-new-link8" after="customer-account-navigation-new-link4">
                <arguments>
                   <argument name="label" xsi:type="string" translate="true">Subscribers</argument>
                    <argument name="path" xsi:type="string">customerecom/account/index</argument>
                </arguments>
            </block>
             <block class="Blink\Customerapi\Block\Customergroup\Ecomlink" name="customer-account-navigation-new-link6" after="customer-account-navigation-new-link3">
