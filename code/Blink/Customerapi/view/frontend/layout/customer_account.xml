                </arguments>
            </block>
             
               <block class="Blink\Customerapi\Block\Customergroup\Ecomlink" name="customer-account-navigation-new-link7" after="customer-account-navigation-new-link6">
                <arguments>
                   <argument name="label" xsi:type="string" translate="true">My Active Plan</argument>
                    <argument name="path" xsi:type="string">customerecom/account/plan</argument>
                </arguments>
            </block>
               
                <block class="Blink\Customerapi\Block\Customergroup\Ecomlink" name="customer-account-navigation-new-link9" after="customer-account-navigation-new-link6">
                <arguments>
                   <argument name="label" xsi:type="string" translate="true">My Reports</argument>
                    <argument name="path" xsi:type="string">customerecom/account/report</argument>
                </arguments>
            </block>
               
            <block class="Blink\Customerapi\Block\Customergroup\Ecomlink" name="customer-account-navigation-new-link5" after="customer-account-navigation-new-link4">
                <arguments>
                   <argument name="label" xsi:type="string" translate="true">Api</argument>
                    <argument name="path" xsi:type="string">customerecom/account/api</argument>
                </arguments>
            </block>
